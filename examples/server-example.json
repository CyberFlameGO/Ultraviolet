{
    "domains": ["localhost", "127.0.0.1", "0.0.0.0"],
    "proxyTo": ":25566",
    "proxyBind": "127.0.0.1",
    "dialTimeout": "1s",
    "sendProxyProtocol": false,
    "disconnectMessage": "Sorry, but the server is offline.",
    "cacheStatus": true,
    "validProtocol": 755,
    "cacheUpdateCooldown": "5s",
    "offlineStatus": {
        "name": "Ultraviolet",
        "protocol": 755,
        "text": "Some offline server"
    },
    "rateLimit": 5,
    "rateCooldown": "4s",
    "stateUpdateCooldown": "10s"
}
